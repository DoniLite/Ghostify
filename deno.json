{
  "tasks": {
    "build": "deno run build:client && deno run tailwind",
    "build:prod": "deno run build && deno run prisma:deploy",
    "build:watch": "deno run build:client:watch && deno run tailwind:watch",
    "start": "deno run build:prod && deno run --allow-net --allow-read --allow-write --allow-env src/server.ts",
    "dev:server": "deno run build:watch & deno run --watch --allow-net --allow-ffi --allow-sys --allow-read --allow-write --allow-env src/server.ts",
    "test": "deno test --allow-net --allow-read --allow-env",
    "lint": "deno lint --fix --ignore=\"**/*.js\"",
    "fmt": "deno fmt",
    "build:client": "deno run --allow-read --allow-write --allow-env --allow-net --allow-run scripts/client/build.ts",
    "build:client:watch": "deno run -A scripts/client/build.ts --watch",
    "tailwind": "deno run -A npm:@tailwindcss/cli -i ./main.css -o ./src/public/css/main.css",
    "tailwind:watch": "deno run --watch -A npm:@tailwindcss/cli -i ./main.css -o ./src/public/css/main.css --watch",
    "setup:assets": "deno run --allow-read --allow-write --allow-run scripts/assets.ts",
    "create:dirs": "deno run --allow-read --allow-write scripts/dir.ts",
    "deploy:server": "deno run --allow-read --allow-write --allow-run --allow-net scripts/deploy.ts",
    "prisma:generate": "deno run --allow-run --allow-read --allow-env --allow-write scripts/db/prisma.ts generate",
    "prisma:migrate": "deno run --allow-run --allow-read --allow-env --allow-write scripts/db/prisma.ts migrate-dev",
    "prisma:migrate:name": "deno run --allow-run --allow-read --allow-env --allow-write scripts/db/prisma.ts migrate-dev --name",
    "prisma:deploy": "deno run --allow-run --allow-read --allow-env scripts/db/prisma.ts migrate-deploy",
    "prisma:pull": "deno run --allow-run --allow-read --allow-env scripts/db/prisma.ts db-pull",
    "prisma:push": "deno run --allow-run --allow-read --allow-env scripts/db/prisma.ts db-push",
    "prisma:reset": "deno run --allow-run --allow-read --allow-env scripts/db/prisma.ts migrate-reset",
    "docker:build": "docker build -t test:latest .",
    "docker:start": "docker run -p 3081:3081 test:latest",
    "learn": "deno run --allow-run --allow-read --allow-env --allow-write ./src/ATS/labs.ts"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.5/",
    "@prisma/adapter-pg": "npm:@prisma/adapter-pg@^6.3.1",
    "@types/bcrypt": "npm:@types/bcrypt@^5.0.2",
    "@types/body-parser": "npm:@types/body-parser@^1.19.5",
    "@types/connect-sqlite3": "npm:@types/connect-sqlite3@^0.9.5",
    "@types/cookie-parser": "npm:@types/cookie-parser@^1.4.8",
    "@types/cors": "npm:@types/cors@^2.8.17",
    "@types/ejs": "npm:@types/ejs@^3.1.5",
    "@types/express": "npm:@types/express@^4.17.21",
    "@types/express-session": "npm:@types/express-session@^1.18.1",
    "@types/express-ws": "npm:@types/express-ws@^3.0.5",
    "@types/formidable": "npm:@types/formidable@^3.4.5",
    "@types/i18n": "npm:@types/i18n@^0.13.12",
    "@types/jsdom": "npm:@types/jsdom@^21.1.7",
    "@types/jsonwebtoken": "npm:@types/jsonwebtoken@^9.0.8",
    "@types/passport": "npm:@types/passport@^1.0.17",
    "@types/passport-google-oauth20": "npm:@types/passport-google-oauth20@^2.0.16",
    "@types/pg": "npm:@types/pg@^8.11.11",
    "@types/showdown": "npm:@types/showdown@^2.0.6",
    "@types/vhost": "npm:@types/vhost@^3.0.9",
    "@types/ws": "npm:@types/ws@^8.5.14",
    "@types/yargs": "npm:@types/yargs@^17.0.33",
    "cookie-parser": "npm:cookie-parser@^1.4.7",
    "express": "npm:express@4.21.2",
    "body-parser": "npm:body-parser@1.20.3",
    "cors": "npm:cors@2.8.5",
    "glob": "npm:glob@^11.0.1",
    "helmet": "npm:helmet@8.0.0",
    "express-rate-limit": "npm:express-rate-limit@7.5.0",
