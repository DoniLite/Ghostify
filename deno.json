    "std/": "https://deno.land/std@0.220.1/",
    "oak": "https://deno.land/x/oak@v12.6.1/mod.ts",
    "djwt": "https://deno.land/x/djwt@v3.0.1/mod.ts",
    "postgres": "https://deno.land/x/postgres@v0.17.0/mod.ts"
  },
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.ns",
      "dom"
    ],
    "strict": true,
    "types": [
      "./src/@types/index.d.ts",
      "./src/@types/session.d.ts"
    ],
    "checkJs": true
  },
  "fmt": {
    "files": {
      "include": [
        "src/"
      ]
    },
    "options": {
      "useTabs": false,
      "lineWidth": 80,
      "indentWidth": 2,
      "singleQuote": true,
      "proseWrap": "preserve"
    }
  },
  "lint": {
    "files": {
      "include": [
        "src/"
      ]
    },
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "permissions": {
    "net": true,
    "read": true,
    "write": true,
    "env": true,
    "run": true
  },
  "nodeModulesDir": "auto"
}
