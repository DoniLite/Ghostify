  "scripts": {
    "build": "pnpm tailwind && pnpm prisma && pnpm deployDB && pnpm pullDB && pnpm generate &&  pnpm exec tsc --outDir build && pnpm setupAssets && make setup && pnpm createDirs",
    "serve": "concurrently --kill-others \"pnpm tailwindDev\" \"tsc --watch\" \"nodemon ./temp/server.js\"",
    "start": "node ./build/server.js",
    "tailwind": "postcss ./src/input.css -o ./src/public/output.css",
    "tailwindDev": "postcss  ./src/input.css -o ./src/public/output.css --watch",
    "setupAssets": "bash ./cmd/assets.sh ./src/public",
    "installSH": "bash ./cmd/install.sh",
    "createDirs": "bash ./cmd/dir.sh",
    "deployServer": "bash ./cmd/deploy.sh",
    "generate": "pnpx prisma generate",
    "migrate": "pnpx prisma migrate dev",
    "prisma": "pnpm dlx prisma",
    "pullDB": "pnpx prisma db pull",
    "deployDB": "prisma migrate deploy",
    "resetDB": "pnpx prisma migrate reset",
    "push": "pnpx prisma db push",
    "test": "pnpm exec playwright test",
    "show-reporter": "pnpm exec playwright show-report",
    "lint": "pnpm eslint",
    "dockerBuild": "docker build -t test:latest .",
    "dockerStart": "docker run -p 3081:3081 test:latest",
    "report": "pnpm exec playwright show-report",
    "learn": "tsc ./src/ATS/labs.ts --skipLibCheck --esModuleInterop --module commonjs --downlevelIteration --target es2021 --moduleResolution node --outDir learn && node ./learn/labs.js --process=train --saveModel=CAPIYERS --dataset=blog_data --epochs=1000"
  },
