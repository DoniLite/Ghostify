{
  "homepage": "https://DoniLite.github.io/doni-blog",
  "devDependencies": {
    "@playwright/test": "^1.45.1",
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.12.7",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.4"
  },
  "scripts": {
    "build": "npm run tailwind && npx prisma migrate deploy && tsc --outDir build",
    "serve": "concurrently --kill-others \"npm run tailwindDev\" \"tsc --watch\" \"nodemon ./temp/server.js\"",
    "start": "node build/server.js",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./src/public/output.css",
    "tailwindDev": "npx tailwindcss -i ./src/input.css -o ./src/public/output.css --watch",
    "deploy": "gh-pages -d build",
    "migrate": "pnpx prisma migrate dev",
    "push": "pnpx prisma db push",
    "test": "pnpm exec playwright test",
    "dbTest": "nodemon ./temp/test.spec.js",
    "show-reporter": "pnpm exec playwright show-report"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/session": "^10.7.0",
    "@fastify/static": "^7.0.3",
    "@fastify/view": "^9.0.0",
    "@prisma/client": "5.15.0",
    "@types/markdown-it": "^14.1.2",
    "apify-client": "^2.9.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "fastify": "^4.26.2",
    "gh-pages": "^6.1.1",
    "markdown-it": "^14.1.0",
    "nodemon": "^3.1.1",
    "p5": "^1.9.2",
    "redis": "^4.6.14",
    "showdown": "^2.1.0",
    "swiper": "^11.1.1"
  },
  "engines": {
    "node": ">=10",
    "pnpm": ">=3"
  },
  "packageManager": "pnpm@9.1.3+sha256.7f63001edc077f1cff96cacba901f350796287a2800dfa83fe898f94183e4f5f"
}
